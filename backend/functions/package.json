{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "serve": "tsc && firebase emulators:start --only functions",
    "serve:reload": "concurrently \"tsc --watch\" \"firebase emulators:start --only functions\"",
    "generate:migration": "ts-node scripts/generateMigration.ts",
    "grantDBPermissions": "ts-node scripts/grantDBPermissions.ts",
    "eas": "ts-node scripts/executeAdminScript.ts",
    "grantAdmin": "ts-node scripts/grantAdmin.ts",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "22"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^0.21.4",
    "express": "^4.17.1",
    "firebase-admin": "^13.2.0",
    "firebase-functions": "^6.3.2",
    "giraffeql": "^2.1.2",
    "image-resizing": "^0.1.3",
    "knex": "^3.1.0",
    "nanoid": "^3.1.30",
    "pg": "^8.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^22.1.0",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "firebase-functions-test": "^0.1.7",
    "fs": "0.0.1-security",
    "prettier": "^2.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "yargs": "^17.3.1"
  },
  "private": true
}
