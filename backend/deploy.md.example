# get current environment config

firebase functions:config:get

# set github config

firebase functions:config:set github.token="ghp_token" github.repository="giraffeql-boilerplate"

# setting db config

firebase functions:config:set pg.user="user" pg.password="password" pg.database="database" pg.host="host"

# set base config

firebase functions:config:set base.origins="https://example.com" base.timeout_seconds="60"

# set serveImage config

firebase functions:config:set serve_image.bucket="x.appspot.com" serve_image.source_path="source" serve_image.cache_path="cache" serve_image.temp_path="temp" serve_image.cdn_url="https://cdn.giraffeql.com"

# set stripe config

firebase functions:config:set stripe.secret_key="sk_" stripe.site_url="https://boilerplate.giraffeql.com"

# delete
firebase functions:config:unset a.b

# env.json file must be in utf-8 encoding
